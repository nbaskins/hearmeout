// the references for these functions are in page of the reference manual 1104
void HAL_TIM_PWM_PulseFinishedCallback (TIM_HandleTypeDef * htim){
	HAL_ADC_Start_DMA(&hadc1, &pwmData[BUFFER_SIZE/2], BUFFER_SIZE/2);
}

void HAL_TIM_PWM_PulseFinishedHalfCpltCallback (TIM_HandleTypeDef * htim){
	HAL_ADC_Start_DMA(&hadc1, &pwmData[0], BUFFER_SIZE/2);
}

void HAL_ADC_ConvCpltCallback(ADC_HandleTypeDef* hadc) {
    // DMA buffer full
}

