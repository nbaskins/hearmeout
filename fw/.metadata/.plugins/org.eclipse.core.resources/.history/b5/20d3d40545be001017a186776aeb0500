/* USER CODE BEGIN PFP */

void formatBuffer(uint16_t volatile* buf, uint32_t size, float start, float end){
	float delta = (end - start) / size;
	for(uint32_t i = 0; i < size; ++i){
		buf[i] = (uint16_t)(start * TICKS_PER_PERIOD);
		start += delta;
	}
}

